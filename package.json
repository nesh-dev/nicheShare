{
  "name": "myApp",
  "version": "1.0.0",
  "description": "",
  "main": "./src/index.js",
  "scripts": {
    "build-server": "babel src --out-dir dist -s",
    "clean": "rm -rf dist && mkdir dist",
    "build": "yarn run clean && yarn run build-server",
    "start": "NODE_ENV=production yarn run build && yarn run serve",
    "start:dev": "NODE_ENV=development  DEBUG=log nodemon --exec babel-node src/index.js",
    "lint": "node_modules/.bin/eslint ./src",
    "db:seed": "node_modules/.bin/sequelize db:seed:all $* --seeders-path src/database/seeders/default",
    "db:seed:unittest": "node_modules/.bin/sequelize db:seed:all $* --seeders-path src/database/seeders/unittesting",
    "db:seed:e2e": "node_modules/.bin/sequelize db:seed:all $* --seeders-path src/database/seeders/e2e",
    "db:migrate": "node_modules/.bin/sequelize db:migrate",
    "db:migrate:undo": "node_modules/.bin/sequelize db:migrate:undo",
    "db:rollback": "node_modules/.bin/sequelize db:migrate:undo:all",
    "db:rollmigrate": "yarn db:rollback && yarn db:migrate && yarn db:seed && babel-node migrator/fetchData",
    "db:user:migrate": "babel-node migrator/fetchData",
    "db:generate": "node_modules/sequelize-auto-migrations/bin/makemigration --name",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@graphql-tools/schema": "^6.0.9",
    "@sendgrid/mail": "^6.4.0",
    "apollo-server": "^2.14.2",
    "apollo-server-express": "^2.14.2",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "bcrypt": "^3.0.7",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dataloader": "^2.0.0",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "eslint": "^6.7.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "express-jwt": "^6.0.0",
    "graphql": "^14.6.0",
    "graphql-constraint-directive": "^2.0.1",
    "graphql-middleware": "^4.0.2",
    "graphql-shield": "^7.0.2",
    "graphql-tools": "^4.0.6",
    "graphql-validation": "^2.2.2",
    "http": "0.0.0",
    "install": "^0.13.0",
    "jsonwebtoken": "^8.5.1",
    "merge-graphql-schemas": "^1.7.6",
    "morgan": "^1.9.1",
    "nodemailer": "^6.4.2",
    "nodemon": "^2.0.1-alpha.3",
    "passport": "^0.4.0",
    "passport-google-oauth": "^2.0.0",
    "passport-google-token": "^0.1.2",
    "passport-jwt": "^4.0.0",
    "pg": "^8.2.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.12",
    "sequelize-auto": "^0.7.1",
    "sequelize-auto-migrations": "^1.0.3",
    "sequelize-automatic-migrations": "^1.0.1",
    "sequelize-cli": "^5.5.1",
    "slugify": "^1.4.0"
  }
